<Window x:Class="AttachedProperties.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:cntrl="clr-namespace:AttachedProperties"
        Title="MainWindow" Height="350" Width="525" Button.Click="Button_Click">
    <Grid>
        <StackPanel>
            <Button x:Name="button1">Button1</Button>
            <Button x:Name="button2">Button2</Button>
            <cntrl:MyControl Height="100" MouseWheel="MyControl_MouseWheel" Background="Blue" BorderBrush="Black" BorderThickness="4"></cntrl:MyControl>
        </StackPanel>
    </Grid>
</Window>
